 ymin = 0.00, 0.00, 0.00, 0.0020, 1.60, 0.00
 xmax = 1.00, 1.00, 1.00, 1.0000, 1.00, 1.00
 ymax = 1.05, 1.05, 1.05, 0.0045, 3.05, 1.05
