#----------------------------------------+
# SOD'S PROBLEM                          |
#                                        |
# (x,t) = (0.50,0.25), (0 <= x <= 1.0)   |
#                                        |
# Wl = Euler1D_pState(1.000,0.0,1.00)    |
# Wr = Euler1D_pState(0.125,0.0,0.10)    |
#                                        |
#----------------------------------------+

# PROBLEM PARAMETERS:
&problem
 i_problem = 1,
 num_cells = 100,
 num_ghost = 2,
 i_gas_type = 0,
/

# TEMPORAL DISCRETIZATION:
&temporal
 i_explicit = 2
 n_stage = 4
 cfl_number = 0.25
 time_max = 0.20
 max_time_steps = 10000
 i_time_step = 0
 i_output_freq = 10
/

# SPATIAL DISCRETIZATION:
&spatial
 i_flux = 3
 i_limiter = 0
 i_recon = 1
/
# Godunov = 0
